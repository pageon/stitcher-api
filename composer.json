{
    "name": "pageon/stitcher-api",
    "description": "Expose Stitcher via a RESTful API",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "BrenDt",
            "email": "brent@pageon.be"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.0",
        "brendt/stitcher": "dev-master",
        "nikic/fast-route": "^1.2",
        "guzzlehttp/psr7": "^1.4"
    },
    "autoload": {
        "psr-4" : {
            "Pageon\\Stitcher\\" : "src/Pageon/Stitcher"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pageon\\Test\\": "tests/Pageon/Test"
        }
    },
    "require-dev": {
        "larapack/dd": "^1.1",
        "phpunit/phpunit": "^6.1"
    }
}
